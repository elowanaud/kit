{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"dev": {
			"with": ["@kit/backend#docker-compose"],
			"cache": false,
			"persistent": true
		},
		"docker-compose": {
			"cache": false,
			"persistent": true
		},
		"tsc": {
			"dependsOn": ["^tsc"]
		}
	}
}
